    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
&mt LCTRL ESC  &kp Q  &kp W  &kp E    &kp R    &kp T             /**/  &kp Y             &kp U    &kp I      &kp O    &kp P     &kp BSPC
&kp TAB        &kp A  &kp S  &kp D    &kp F    &kp G             /**/  &kp H             &kp J    &kp K      &kp L    &kp SEMI  &tdc_tm
&kp LALT       &kp Z  &kp X  &kp C    &kp V    &kp B             /**/  &kp N             &kp M    &kp COMMA  &kp DOT  &kp FSLH  &kp RALT
                             &tdc_sp  &moto_l  &mt LSHIFT SPACE  /**/  &mt RSHIFT ENTER  &moto_r  &kp LCTRL
            >;
        };

        lower_layer {
            bindings = <
&mt LCTRL ESC  &kp N1        &kp N2        &kp N3        &kp N4        &kp N5            /**/  &kp N6            &kp N7    &kp N8     &kp N9           &kp N0     &kp BSPC
&kp TAB        &bt BT_SEL 0  &bt BT_SEL 1  &bt BT_SEL 2  &bt BT_SEL 3  &bt BT_SEL 4      /**/  &kp LEFT          &kp DOWN  &kp UP     &kp RIGHT        &kp HOME   &kp END
&kp LALT       &bt BT_SEL 5  &bt BT_SEL 6  &out OUT_BLE  &out OUT_TOG  &bt BT_CLR        /**/  &kp SLCK          &kp INS   &kp PSCRN  &kp PAUSE_BREAK  &kp PG_UP  &kp PG_DN
                                           &kp LGUI      &trans        &mt LSHIFT SPACE  /**/  &mt RSHIFT ENTER  &trans    &kp LCTRL
            >;
        };

        raise_layer {
            bindings = <
&mt LCTRL ESC  &kp EXCL  &kp AT      &kp HASH      &kp DLLR      &kp PRCNT         /**/  &kp CARET         &kp AMPS   &kp ASTRK  &kp LPAR  &kp RPAR  &kp DEL
&kp TAB        &kp CLCK  &kp C_MUTE  &kp C_VOL_DN  &kp C_VOL_UP  &none             /**/  &kp MINUS         &kp EQUAL  &kp LBKT   &kp RBKT  &kp BSLH  &kp GRAVE
&kp LALT       &none     &none       &kp C_BRI_DN  &kp C_BRI_UP  &none             /**/  &kp UNDER         &kp PLUS   &kp LBRC   &kp RBRC  &kp PIPE  &kp TILDE
                                     &kp LGUI      &trans        &mt LSHIFT SPACE  /**/  &mt RSHIFT ENTER  &trans     &kp LCTRL
            >;
        };

        both_layer {
            bindings = <
&kp F1  &kp F2  &kp F3  &kp F4    &kp F5  &kp F6            /**/  &kp F7            &kp F8  &kp F9     &kp F10  &kp F11  &kp F12
&none   &none   &none   &none     &none   &none             /**/  &none             &none   &none      &none    &none    &none
&none   &none   &none   &none     &none   &none             /**/  &none             &none   &none      &none    &none    &tog 4
                        &kp LGUI  &trans  &mt LSHIFT SPACE  /**/  &mt RSHIFT ENTER  &trans  &kp LCTRL
            >;
        };

        calc_layer {
            bindings = <
&kp ESC   &none   &none   &none     &none   &none             /**/  &none             &kp N7  &kp N8  &kp N9  &kp KP_DIVIDE    &kp BSPC 
&kp TAB   &none   &none   &none     &none   &none             /**/  &none             &kp N4  &kp N5  &kp N6  &kp KP_MULTIPLY  &kp KP_MINUS 
&kp LALT  &none   &none   &none     &none   &none             /**/  &kp N0            &kp N1  &kp N2  &kp N3  &kp KP_DOT       &kp KP_PLUS
                          &kp LGUI  &tog 4  &mt LSHIFT SPACE  /**/  &mt RSHIFT ENTER  &tog 5  &kp LCTRL
            >;
        };

        dual_layer {
            bindings = <
&mt LCTRL ESC  &tdc_q  &tdc_w  &tdc_e   &kp R    &tdc_t       /**/  &tdc_y            &tdc_u   &tdc_i     &tdc_o   &kp P     &kp BSPC
&kp TAB        &tdc_a  &tdc_s  &kp D    &kp F    &kp G        /**/  &kp H             &kp J    &kp K      &kp L    &kp SEMI  &tdc_tm
&kp LALT       &tdc_z  &tdc_x  &tdc_c   &tdc_v   &tdc_b       /**/  &tdc_n            &kp M    &kp COMMA  &kp DOT  &kp FSLH  &kp RALT
                          &kp LGUI  &tog 5  &mt LSHIFT SPACE  /**/  &mt RSHIFT ENTER  &trans  &kp LCTRL
            >;
        };
    };

    conditional_layers {
        compatible = "zmk,conditional-layers";

        both {
            if-layers = <1 2>;
            then-layer = <3>;
        };
    };
};
