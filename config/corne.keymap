        moto_l: mo_to_l {
            compatible = "zmk,behavior-mod-morph";
            #binding-cells = <0>;
            bindings = <&mo 1>, <&tog 1>;
            mods = <(MOD_RSFT)>;
        };

        moto_r: mo_to_r {
