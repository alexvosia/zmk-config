    behaviors {
        tdc_q: tap_dance_q {
            compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&kp Q>, <&zed_q>;
        };

        tdc_w: tap_dance_w {
            compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&kp W>, <&zed_w>;
        };

        tdc_t: tap_dance_t {
            compatible = "zmk,behavior-tap-dance";
